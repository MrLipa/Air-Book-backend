{
  "name": "air-book-backend",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.js",
  "scripts": {
    "dev": "nodemon --legacy-watch src/server.js",
    "start": "node src/server.js",
    "test:integration": "npx mocha --file tests/integration/setup.mjs 'tests/integration/**/*.test.mjs' --reporter mochawesome --reporter-options reportDir=logs,reportFilename=test-report",
    "test:performance": "k6 run tests/performance/main.test.js",
    "test:unit": "npx mocha tests/unit/**/*.test.mjs --reporter mochawesome --reporter-options reportDir=logs,reportFilename=test-report",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write 'src/**/*.{js,ts,mjs,cjs}'",
    "docs": "typedoc"
  },
  "author": "Tomasz Szkaradek",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.10.0",
    "bcrypt": "^5.1.0",
    "chai": "^5.2.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "*",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "k6": "^0.0.0",
    "mysql2": "^3.14.1",
    "neo4j-driver": "*",
    "node-cron": "^4.0.7",
    "nodemon": "^3.1.10",
    "prom-client": "^15.0.0",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "globals": "^16.2.0",
    "mocha": "^10.2.0",
    "mochawesome": "^7.1.3",
    "prettier": "^3.5.3",
    "supertest": "^6.3.3",
    "typedoc": "^0.28.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
